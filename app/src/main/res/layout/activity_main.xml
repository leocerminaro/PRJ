<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <FrameLayout
            android:id="@+id/container"
            android:layout_width="match_parent"
            android:layout_height="723dp"
            app:layout_behavior="@string/appbar_scrolling_view_behavior" tools:ignore="MissingConstraints"/>

    <android.support.design.widget.BottomNavigationView
            android:id="@+id/navigationView"
            android:layout_width="411dp"
            android:layout_height="wrap_content"
            android:background="?android:attr/windowBackground"
            app:itemBackground="@color/colorPrimary"
            app:menu="@menu/navigation" tools:layout_editor_absoluteY="675dp" tools:ignore="MissingConstraints"/>

        <FrameLayout android:layout_width="wrap_content" android:layout_height="644dp" tools:ignore="MissingConstraints">
        <fragment
        android:id="@+id/map"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:layout_width="408dp"
        android:layout_height="match_parent" tools:layout_editor_absoluteX="3dp"/>
        </FrameLayout>



        </android.support.constraint.ConstraintLayout>